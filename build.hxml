-cp src
-cp haxe-lsp/shared

-lib haxeserver
-lib hxargs
-lib safety
-lib hxnodejs

-D analyzer-optimize

--each

-cp haxeserver/haxe-lsp/src
-lib language-server-protocol
-lib vscode-json-rpc

-D haxeserver.repro.displayrequests_wait
-D haxeserver.repro.displayrequests_delay=100

-main haxeserver.repro.ReplayRecording
--js bin/replay.js

--next

-main haxeserver.repro.ReduceRecording
--js bin/reduce.js
